int Pipe(char * args[]){
	int a = 0; 
	int b = 0; 
	
	int background = 0; 
	char *args1[256]; 
	char *cmd = Convert(args); 
	
	
	if(background != -1){
		argv = RemoveArr(args, background); 
	}
	
	while(args[a] != NULL && background ==0){
		if(strcmp(args[a], "&") == 0){
			background = 1; 
		}
		else if(strcmp(args[a], "|") == 0){
			pipe(args); 
			return 1; 
		}
	}
}
