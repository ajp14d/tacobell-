
CHANGES MADE TO UTILITY FILES:

BigFree --> void MemFunc(char** arr);

PrintArgVector --> void DisplayArgs(char** args);

DeleteCharacter --> char* DelFunc(char* line, size_t begin, size_t end);

FileExist --> int ExistCheck(const char* file);

IsExecutable --> int ExecCheck(const char* file);

DynStrCat --> char* LinkString(char* x, const char* y);

DynStrPushBack --> char* BPushString(char* x, char y);

DynStrPushFront --> char* FPushString(char* x, char y);

RemoveLastDir --> char* RmDir(char* p);

IsCommand --> int CmdCheck(char** args, int a);

BuildPath --> char* PathMaker(char* s);

PathEnvBuildPath --> char* PathFromEVar(char* s);

ValidExternalCommand --> int CmdVal(const char* s);

CleanPathDirectory --> char* PathClear(char* s);

ReplaceSubStr --> char* CharRep(char* x, size_t begin, size_t end, const char* y);

ContainsChar --> int CharCheck(const char* s, char c);

VecContainsStr --> int StringCheck(char** argv, const char* s);

ArraySize --> int GetSize(char** argv);

ArrayPushBack --> char** PBackArr(char** argv, const char* s);

ArrayRemoveElement --> char** RemoveArr(char** argv, int a);

ArgvToString --> char* Convert(char** argv);

ArgvCountSymbol --> int CountStr(char** argv, const char* s);
